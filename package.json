{
  "$schema": "http://json.schemastore.org/package",
  "private": true,
  "name": "@chacha-26/unidecode",
  "version": "1.0.0",
  "description": "Lossy Unicode -> Ascii converter. Based on the python Unidecode package.",
  "main": "dist/bundle.js",
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^8.0.41",
    "ava": "^0.22.0",
    "nyc": "^11.2.1",
    "rollup": "^0.50.0",
    "tslint": "^5.7.0",
    "tslint-language-service": "^0.9.6",
    "typescript": "^2.5.3"
  },
  "scripts": {
    "prebuild": "tsc -p tsconfig.data.json && node build/generate_data_from_py.js",
    "build": "tsc -p tsconfig.build.json",
    "postbuild": "rollup build/index.js --output.name unidecode --output.format iife --output.file dist/bundle.js",
    "pretest": "tsc -p tsconfig.test.json",
    "test": "nyc --reporter=lcov ava build/test.js",
    "posttest": "nyc report"
  },
  "author": "Chacha-26 <chacha-26@protonmail.com>",
  "license": "ISC"
}
